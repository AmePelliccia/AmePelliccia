from music21 import stream, note, chord

# Crear una secuencia musical básica
s = stream.Stream()

# Añadir notas y acordes
s.append(note.Note("C4"))
s.append(chord.Chord(["E4", "G4"]))
s.append(note.Note("A4"))
s.append(chord.Chord(["F4", "A4", "C5"]))

# Exportar como archivo MIDI
s.write('midi', fp='crypto_music.mid')


 




